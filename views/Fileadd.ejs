    <h3>Ajouter un nouveau Fichier</h3>
    
    <div class="col-12 row justify-content-center">
      <form class="col-8 card box mt-4 " id="form" method="post" action="http://localhost:3000/File/upload" enctype="multipart/form-data">
        <div class="row justify-content-center">
            <div class="row mt-4 justify-content-center">
                <input type="file" name="file" />
            </div>
            <select class="form-controle col-2 text-center mt-4" name="depart" aria-label="Default select example">
                <% Departement.forEach(function(dept){ %>
                <option value=" <%= dept.id %>"><%= dept.dept_name %></option>
                <% }); %>
              </select>
        </div>
      
      <br/>
        <div class="row mt-2 mb-4 justify-content-center">
          <button type="submit" class="btn btn-primary col-4">Ajouter</button>
        </div>
      </form>
    </div>
